@model scratch.ViewModel.MovieViewModel

@{
    ViewBag.Title = "ListMoviesView";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h1> Movies </h1>
<table class="table table-bordered table-hover">
    <thead>
        <tr>
            <th scope="col">Movies</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var movie in Model.movieList)
        {
            <tr>
                <th scope="row">@movie.Name</th>
            </tr>
        }
    </tbody>
</table>


